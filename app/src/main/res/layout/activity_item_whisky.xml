<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".item_whisky">

    <ec.dina.btefonts.TextViewFonts
        android:id="@+id/txtNombre"
        android:layout_width="185dp"
        android:layout_height="37dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:text="Nombre"
        android:textColor="#853C01"
        android:textSize="18sp"
        app:dm_font="segoe_print"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ec.dina.btefonts.TextViewFonts
        android:id="@+id/txtMoneda"
        android:layout_width="185dp"
        android:layout_height="37dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:text="Moneda"
        android:textColor="#853C01"
        android:textSize="18sp"
        app:dm_font="segoe_print"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNombre" />

    <ec.dina.btefonts.TextViewFonts
        android:id="@+id/textView"
        android:layout_width="130dp"
        android:layout_height="37dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="5dp"
        android:text="Nombre: "
        android:textColor="#853C01"
        android:textSize="18sp"
        app:dm_font="segoe_print"
        app:layout_constraintEnd_toStartOf="@+id/txtNombre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ec.dina.btefonts.TextViewFonts
        android:id="@+id/textView2"
        android:layout_width="130dp"
        android:layout_height="37dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="5dp"
        android:text="Tipo Moneda:"
        android:textColor="#853C01"
        android:textSize="18sp"
        app:dm_font="segoe_print"
        app:layout_constraintEnd_toStartOf="@+id/txtMoneda"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />
</androidx.constraintlayout.widget.ConstraintLayout>