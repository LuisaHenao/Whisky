<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".item_destileria">

    <ec.dina.btefonts.TextViewFonts
        android:id="@+id/txtContinente"
        android:layout_width="130dp"
        android:layout_height="37dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:shadowColor="#853C01"
        android:text="continente"
        android:textSize="18sp"
        app:dm_font="segoe_print"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNombreDesti" />

    <ec.dina.btefonts.TextViewFonts
        android:id="@+id/txtNombreDesti"
        android:layout_width="130dp"
        android:layout_height="37dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:shadowColor="#853C01"
        android:text="Nombre"
        android:textSize="18sp"
        app:dm_font="segoe_print"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ec.dina.btefonts.TextViewFonts
        android:id="@+id/textView6"
        android:layout_width="130dp"
        android:layout_height="37dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="5dp"
        android:shadowColor="#853C01"
        android:text="Nombre:"
        android:textSize="18sp"
        app:dm_font="segoe_print"
        app:layout_constraintEnd_toStartOf="@+id/txtNombreDesti"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ec.dina.btefonts.TextViewFonts
        android:id="@+id/textView7"
        android:layout_width="130dp"
        android:layout_height="37dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="5dp"
        android:shadowColor="#853C01"
        android:text="Continente:"
        android:textSize="18sp"
        app:dm_font="segoe_print"
        app:layout_constraintEnd_toStartOf="@+id/txtContinente"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />
</androidx.constraintlayout.widget.ConstraintLayout>